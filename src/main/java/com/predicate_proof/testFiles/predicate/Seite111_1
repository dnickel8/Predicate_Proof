\block_begin
1 \forall x (P(x) -> Q(x)) \formula_end premise \eol
2 \forall x P(x) \formula_end premise \eol
\block_begin
3 y \variable_end P(y) -> Q(y) \formula_end \forall x e 1 \eol
4 P(y) \formula_end \forall x e 2 \eol
5 Q(y) \formula_end -> e 3,4 \eol
\block_end
6 \forall x Q(x) \formula_end \forall x i 3-5 \eol
\block_end