\block_begin
1 \exists x P(x) \formula_end premise \eol
2 \forall x \forall y (P(x) -> Q(y) ) \formula_end premise \eol
\block_begin
3 w \variable_end \eol
\block_begin
4 v \variable_end P(v) \formula_end ass \eol
5 \forall y (P(v) -> Q(y)) \formula_end \forall x e 2 \eol
6 P(v) -> Q(w) \formula_end \forall y e 5 \eol
7 Q(w) \formula_end -> e 6,4 \eol
\block_end
8 Q(w) \formula_end \exists x e 1, 4-7 \eol
\block_end
9 \forall y Q(y) \formula_end \forall y i 3-8 \eol
